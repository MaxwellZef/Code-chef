#include <iostream>
using namespace std;

int main() {
	int l, b;
    scanf("%d %d", &l, &b);
    int area = l*b;
    int perimeter = 2*(l+b);
    if (area > perimeter){
        printf("Area\n");
        printf("%d", area);
    }
    else if (area == perimeter){
        printf("Eq\n");
        printf("%d", area);
    }
    else {
        printf("Peri\n");
        printf("%d", perimeter);
    }
	return 0;
}
